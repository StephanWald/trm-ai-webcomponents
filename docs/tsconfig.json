{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"],
      "@generated/*": ["./.docusaurus/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".docusaurus/**/*.ts",
    ".docusaurus/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
